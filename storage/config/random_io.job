[global]
ioengine=libaio
iodepth=2
direct=1
sync=1
fsync_on_close=1
time_based=1
runtime=30
clocksource=clock_gettime
ramp_time=5
startdelay=5
filename=/var/lib/fio/test
size=1g
write_bw_log=fio
write_iops_log=fio
write_lat_log=fio
write_hist_log=fio
per_job_logs=1
log_avg_msec=60000
log_hist_msec=60000

[fio-1]
bs=4k
rw=randread
numjobs=1

