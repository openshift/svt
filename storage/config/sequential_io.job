[global]
ioengine=libaio
bs=4k, 128k, 4096k
iodepth=4
direct=0
fsync_on_close=1
time_based=1
runtime=5720
clocksource=clock_gettime
ramp_time=10
startdelay=20

[fio]
size=95g
write_bw_log=fio
write_iops_log=fio
write_lat_log=fio
write_hist_log=fio
numjobs=1
per_job_logs=1
log_avg_msec=60000
log_hist_msec=60000